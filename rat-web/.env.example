# GitHub OAuth App Configuration
# Create a new OAuth App at https://github.com/settings/applications/new
# For Device Flow, you only need Client ID and Secret
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Application URL
# For local development, use http://localhost:8788
# For production, use your deployed domain
APP_BASE_URL=http://localhost:8788

# AI Agent Configuration
# Your AI service endpoint for code generation
AI_AGENT_URL=https://api.your-ai-service.com/v1/agent
AI_AGENT_KEY=your_ai_agent_api_key_here

# Cloudflare KV Namespace IDs
# Create these in your Cloudflare dashboard or with wrangler
# wrangler kv:namespace create SESSIONS
# wrangler kv:namespace create CACHE
SESSIONS_KV_NAMESPACE_ID=your_sessions_kv_id_here
SESSIONS_KV_PREVIEW_ID=your_sessions_kv_preview_id_here
CACHE_KV_NAMESPACE_ID=your_cache_kv_id_here
CACHE_KV_PREVIEW_ID=your_cache_kv_preview_id_here

# Optional: Debug mode
DEBUG=true